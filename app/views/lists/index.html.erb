<div class="container">
  <h1>All the lists</h1>

  <ul class="list-group py-3">
    <% @lists.each do |list| %>
      <li class="list-group-item">
        <%= link_to list.name, list_path(list) %>
      </li>
    <% end %>
  </ul>

  <%= link_to "New List", new_list_path, class: "btn btn-primary" %>
</div>
